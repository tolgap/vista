<!DOCTYPE html>
<html>
<head>
  <title>Wordpress Vista</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="wrap">
  <%= render "shared/menu" %>
  <div class="content">
    <div class="container">
      <div class="breadcrumb">
        <%= render_breadcrumbs %>
      </div>
      <%= yield %>
    </div>
  </div>
</div>

</body>
</html>
